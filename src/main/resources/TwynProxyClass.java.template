import TARGET_INTERFACE_QUALIFIED;

public class CLASS_NAME implements TARGET_INTERFACE {
	private Twyn twyn;
	private JsonNode jsonNode;
	
	public CLASS_NAME(Twyn twyn, JsonNode jsonNode) {
		this.jsonNode = jsonNode;
		this.twyn = Objects.requireNonNull(twyn);
	}
	
IMPLEMENTED_METHODS

	private <T> List<T> collect(Class<T> componentType, JsonNode jsonNode) {
		return StreamSupport.stream(jsonNode.spliterator(), false)
			.map(n -> twyn.proxy(n, componentType))
			.collect(Collectors.toList());
	}
	
	public String toString() {
		return "TwynProxyClass [jsonNode=" + jsonNode + "]";
	}
	
}